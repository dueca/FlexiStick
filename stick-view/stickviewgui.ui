<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="stick-draw">
    <property name="name">stick-draw</property>
    <property name="can_focus">False</property>
    <property name="title" translatable="yes">HID feedback</property>
    <child>
      <object class="GtkDrawingArea" id="drawing">
        <property name="name">drawing</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
      </object>
    </child>
  </object>
</interface>
